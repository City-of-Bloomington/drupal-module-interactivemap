<section id="interactivemap_maps">
    <header><h2>Maps</h2></header>
    <div>
    {% for map in maps %}
    <article>
        <a href="{{ map.url }}">
            <header><h3>{{ map.name }}</h3></header>
            <p>{{ map.description }}</p>
        </a>
    </article>
    {% endfor %}
    </div>
</section>
